<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class Cobrador extends Model
{
  protected $table = 'cobrador';
  protected $primaryKey = 'cobrador_id';

  public $timestamps = false;

  protected $fillable = ['cobrador_id','rol_id','cobrador_nombre', 'cobrador_cedula', 'cobrador_direccion'];
}
