<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class Segusuario extends Model
{
  protected $table = 'segusuario';
  protected $primaryKey = 'usuario_id';

  public $timestamps = false;

  protected $fillable = ['usuario_id', 'usuario_username', 'usuario_nombre', 'usuario_email','usuario_contraseña'];
}
